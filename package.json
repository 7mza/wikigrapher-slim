{
  "name": "wikigrapher-slim",
  "version": "0.0.1",
  "description": "wikigrapher public release https://wikigrapher.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/7mza/wikigrapher-slim"
  },
  "license": "AGPL-3.0-or-later",
  "author": "github.com/7mza",
  "scripts": {
    "build": "npm run format && cross-env NODE_ENV=production npx webpack --mode=production",
    "build:dev": "npm run format && cross-env NODE_ENV=development npx webpack --mode=development",
    "clean": "npm cache verify",
    "clear": "npm run clean",
    "ncu": "npx ncu",
    "format": "npx prettier **/*.{cjs,css,html,js,json,mjs,scss,tjs,ts,yaml,yml} --write",
    "lint": "npx eslint"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@popperjs/core": "^2.11.8",
    "@types/bootstrap": "^5.2.10",
    "@types/file-saver": "^2.0.7",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^10.0.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "browserslist": "^4.28.1",
    "core-js": "^3.48.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "eslint": "^9.39.2",
    "file-saver": "^2.0.5",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^13.0.0",
    "globals": "^17.1.0",
    "mini-css-extract-plugin": "^2.10.0",
    "npm-check-updates": "^19.3.1",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "purgecss-webpack-plugin": "^7.0.2",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vis-network": "^10.0.2",
    "webpack": "^5.104.1",
    "webpack-assets-manifest": "^6.5.0",
    "webpack-cli": "^6.0.1"
  },
  "prettier": {
    "attributeSort": "ASC",
    "bracketSameLine": true,
    "plugins": [
      "prettier-plugin-organize-attributes",
      "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "singleAttributePerLine": true
  },
  "browserslist": [
    ">0.3%",
    "defaults",
    "fully supports es6-module",
    "not dead"
  ]
}
